#!/bin/bash
python main.py --config_file configs/imagenet.yaml --mask_aware --pixel_based --outdir ./images/ablation/imagenet/pixel_LPIPS/20 --optimize_xt.lr_xt_decay 1 --optimize_xt.num_iteration_optimize_xt 1 --optimize_xt.loss LPIPS --mask_type 20 --device 0
